from "string.cpl" import itoa reverse;
from "memory.cpl" import mm_init malloc free memset;
from "stdio.cpl" import gets prints;

start
    ptr char input = malloc(36);
    memset(input, 0, 35);
    gets(input, 35);
    prints(input);

    ptr char buffer = malloc(9);
    memset(buffer, 0, 8);

    int digit_len = itoa(buffer, 8, 1234567);
    reverse(buffer, digit_len - 1);

    prints(buffer);
    prints(input);
    free(buffer);
    free(input);
exit 0;